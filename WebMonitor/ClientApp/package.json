{
  "name": "webmonitor",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "node aspnetcore-https",
    "start": "run-script-os",
    "start:windows": "ng serve --port 44427 --ssl --ssl-cert \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.key\"",
    "start:default": "ng serve --port 44427 --ssl --ssl-cert \"$HOME/.aspnet/https/${npm_package_name}.pem\" --ssl-key \"$HOME/.aspnet/https/${npm_package_name}.key\"",
    "build": "ng build",
    "build:ssr": "ng run WebMonitor:server:dev",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.2",
    "@angular/cdk": "^20.0.1",
    "@angular/common": "^20.3.2",
    "@angular/compiler": "^20.2.3",
    "@angular/core": "^20.3.2",
    "@angular/forms": "^20.2.3",
    "@angular/material": "^20.2.5",
    "@angular/platform-browser": "^20.2.3",
    "@angular/platform-browser-dynamic": "^20.3.2",
    "@angular/platform-server": "^20.2.3",
    "@angular/router": "^20.2.3",
    "bootstrap": "^5",
    "jquery": "^3",
    "oidc-client": "^1.11.5",
    "popper.js": "^1.16.0",
    "run-script-os": "^1.1.6",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1",
    "xterm": "^5.2.1",
    "xterm-addon-attach": "^0.8.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-web-links": "^0.8.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.2.1",
    "@angular/cli": "^20.3.3",
    "@angular/compiler-cli": "^20.2.3",
    "@types/jasmine": "~5.1.9",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^24.6.1",
    "jasmine-core": "~5.10.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "typescript": "^5.9.2"
  },
  "overrides": {
    "autoprefixer": "10.4.5"
  }
}